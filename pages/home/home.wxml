<view class="home-container">
    <scroll-view class="todoList" scroll-y="true">

    </scroll-view>
    <t-button type="change" class="createButton" text="创建任务">创建任务</t-button>
    <view class="tomato-wrapper">
        <view class="tomato">
            <t-clock text="点击开始闹钟"></t-clock>
        </view>
    </view>
    <g-confirm visible="{{visible}}" placeholder="输入点什么"
        bindconfirm="confirm" bindcancel="cancel"></g-confirm>
</view>
